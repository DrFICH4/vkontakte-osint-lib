{
    "name": "postuf/vkontakte-api-lib",
    "description": "Vkontakte scenario-based API aimed at OSINT",
    "homepage": "https://github.com/Postuf/vkontakte-api-lib",
    "license": "MIT",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-curl": "*",
        "phpseclib/phpseclib": "^2.0",
        "vkcom/vk-php-sdk": "^5.101"
    },
    "autoload": {
        "psr-4": {
            "VkontakeOSINT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": ["tests/", "examples/"]
        }
    },
    "prefer-stable": true
}